<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:background="@drawable/summer_generic">

    <TextView
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:text="DATA KAMUS"
            android:id="@+id/textView18" android:layout_alignParentTop="true" android:layout_centerHorizontal="true"
            android:textStyle="bold|italic" android:background="@drawable/abc_textfield_activated_mtrl_alpha"
            android:gravity="center" android:textColor="#009900"/>
    <Button
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="@string/btnKembaliLihat"
            android:id="@+id/btnKembaliLihat" android:layout_below="@+id/textView18" android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" android:textColor="#999900"
            android:background="@drawable/abc_popup_background_mtrl_mult"/>
    <ListView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/listLiahatKamus" android:layout_below="@+id/btnKembaliLihat" android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"/>
</RelativeLayout>